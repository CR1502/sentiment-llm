{
  "family": "sentiment-api-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sentiment-api",
      "image": "694819981194.dkr.ecr.us-east-2.amazonaws.com/sentiment-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}